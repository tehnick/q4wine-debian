#!/usr/bin/make -f

export DEB_LDFLAGS_MAINT_APPEND = -Wl,--as-needed
export DEB_BUILD_MAINT_OPTIONS = hardening=+all

CMAKEOPTS = -DLIBS_ENTRY_PATH=/usr/lib/q4wine/ \
            -DQT5=ON \
            -DUSE_GZIP=ON \
            -DWITH_WINEAPPDB=OFF

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -- $(CMAKEOPTS)
